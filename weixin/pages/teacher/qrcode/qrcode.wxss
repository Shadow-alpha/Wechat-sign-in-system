/* pages/teacher/qrcode/qrcode.wxss */
/* 签到发起部分 */
.form{
    position:absolute;
    bottom: 0;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    height: 600rpx;
    background-color: #eee;
}

.form .picker{
    height: 80rpx;
    width: 500rpx;
    border: 1px solid #ccc;
    background-color: #fff;
    margin-bottom: 10rpx;
    line-height: 80rpx;
    padding: 0 15rpx;
}
.form::before{
    content: "发布签到二维码";
    position: absolute;
    top: 0;
    left: 0;
    font-weight: 800;
}
